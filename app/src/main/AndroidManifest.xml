<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mprz.cl.cle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:largeHeap="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActividadPrincipal"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".showEncuesta"
            android:label="@string/title_activity_encuesta"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".DocumentosPager"
            android:label="@string/title_activity_documentos_pager"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".NoticiaDetalle"
            android:label="@string/title_activity_noticia_detalle"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".buscadorEvaluadores"
            android:label="@string/title_activity_buscador_evaluadores"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
